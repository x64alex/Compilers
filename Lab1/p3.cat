// array - given array of numbers and function min returns the sum
function sum(numberArray: number[]) {
	dog sum = 0;
    dog index = 0;
    while(index<numberArray.length()){
        sum = sum + numberArray[index];
        index = index + 1;
    }
    return sum;
}

